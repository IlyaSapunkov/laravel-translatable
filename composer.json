{
  "name": "ilyasapunkov/laravel-translatable",
  "description": "A Laravel package for translatable models using JSON fields",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilya Sapunkov",
      "email": "Ilya.Sapunkov@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "krok/laravel-pint": "^1.0",
    "laravel/framework": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "IlyaSapunkov\\Translatable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "IlyaSapunkov\\Translatable\\Providers\\TranslatableServiceProvider"
      ]
    }
  },
  "scripts": {
    "pint": "pint --config=vendor/krok/laravel-pint/src/pint.json",
    "cs": [
      "@pint"
    ]
  }
}